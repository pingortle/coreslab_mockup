<h1>WorkArea</h1>

<table>
  <tr>
    <td>
      <strong>Name:</strong>
    </td>
    <td>
      {{model.name}}
    </td>
  </tr>
  <tr>
    <td>
      <strong>Subareas:</strong>
    </td>
    <td>
      <ul>
        {{#each model.subareas}}
          <li>{{this.name}}</li>
        {{/each}}
      </ul>
    </td>
  </tr>
</table>

{{#link-to 'workarea.edit' model}}Change{{/link-to}}

{{outlet}}
